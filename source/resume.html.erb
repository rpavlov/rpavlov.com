<% blog.articles.select {|a| a.data[:resume]}.each do |article| %>
<article class="post resume">
  <h1 class="post-title"><%= article.title.to_str %></h1>
  <div class="post-meta"><%= article.date.strftime('%e %B %Y') %></div>
  <div class="post-text">
    <%= article.body %>
  </div>
</article>
<% end %>
